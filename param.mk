

#models_list
DDEFS           += -DROBOT_DIFF_2WD=1 -DROBOT_DIFF_4WD=2 -DROBOT_OMNI_3=10 -DROBOT_OMNI_4=11 -DROBOT_MECANUM=12

#motor_controller_list
DDEFS           += -DCOMMON_CONTROLLER=1 -DAF_SHIELD_CONTROLLER=2

DDEFS			+= -DMOTOR1_REVERSE=0 -DMOTOR2_REVERSE=0 -DMOTOR3_REVERSE=0 -DMOTOR4_REVERSE=0
DDEFS			+= -DENCODER1_REVERSE=0 -DENCODER2_REVERSE=0 -DENCODER3_REVERSE=0 -DENCODER4_REVERSE=0 
#models
#DDEFS           += -DROBOT_MODEL=ROBOT_DIFF_2WD -DMOTOR_CONTROLLER=COMMON_CONTROLLER
