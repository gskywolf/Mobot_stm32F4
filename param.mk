DDEFS           += -DROBOT_MODEL=ROBOT_MECANUM -DMOTOR_CONTROLLER=COMMON_CONTROLLER


#com
DDEFS           += -DUSART_DEBUG=USART_2 -DMASTER_USART=USART_0
